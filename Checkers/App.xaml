<Application x:Class="Checkers.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Checkers"
             StartupUri="Lobby.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <BitmapImage x:Key="kamen-red" UriSource="Images\kamen-red.png"/>
            <BitmapImage x:Key="kamen-blue" UriSource="Images\kamen-blue.png"/>
            <BitmapImage x:Key="kamen-red-queen" UriSource="Images\kamen-red-queen.png"/>
            <BitmapImage x:Key="kamen-blue-queen" UriSource="Images\kamen-red-queen.png"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
